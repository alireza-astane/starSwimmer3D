    cylinder { m*<-4.714045207910317,8.16496580927726,-2.867570011501037>, <-4.385255000899801e-18,-5.038759458414085e-19,0.465763321832295>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.867570011501037>, <-4.385255000899801e-18,-5.038759458414085e-19,0.465763321832295>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
