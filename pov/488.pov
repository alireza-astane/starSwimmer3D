    cylinder { m*<-4.714045207910317,8.16496580927726,-2.717961288523089>, <-3.5195452983143265e-18,-2.961604050943513e-18,0.6153720448102449>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.717961288523089>, <-3.5195452983143265e-18,-2.961604050943513e-18,0.6153720448102449>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
