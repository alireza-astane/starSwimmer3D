    cylinder { m*<-4.714045207910317,8.16496580927726,-2.8083462715173324>, <-3.801053503470944e-18,-2.3341403428122247e-19,0.5249870618160006>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.8083462715173324>, <-3.801053503470944e-18,-2.3341403428122247e-19,0.5249870618160006>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
