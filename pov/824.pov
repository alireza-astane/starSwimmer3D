    cylinder { m*<-4.714045207910317,8.16496580927726,-2.325275218104939>, <-1.3526344144686655e-18,-5.0027165103403e-18,1.0080581152283934>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.325275218104939>, <-1.3526344144686655e-18,-5.0027165103403e-18,1.0080581152283934>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
    finish { ambient 0.1 diffuse 0.7 phong 1 }    
