    cylinder { m*<-4.714045207910317,8.16496580927726,-2.9609734114117052>, <-5.687350767789434e-18,7.327275475500627e-19,0.3723599219216271>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.9609734114117052>, <-5.687350767789434e-18,7.327275475500627e-19,0.3723599219216271>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
