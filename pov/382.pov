    cylinder { m*<-4.714045207910317,8.16496580927726,-2.8477789558864486>, <-4.1330600032999965e-18,-2.2162811737553663e-20,0.48555437744688323>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.8477789558864486>, <-4.1330600032999965e-18,-2.2162811737553663e-20,0.48555437744688323>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
