    cylinder { m*<-4.714045207910317,8.16496580927726,-3.0338821594401217>, <-4.528188143115817e-18,4.702083803570152e-19,0.2994511738932108>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-3.0338821594401217>, <-4.528188143115817e-18,4.702083803570152e-19,0.2994511738932108>, 0.5}

