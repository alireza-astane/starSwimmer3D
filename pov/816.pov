#version 3.7; 
    global_settings { assumed_gamma 1.0 }
    

    camera {
    location  <20, 20, 20>
    right     x*image_width/image_height
    look_at   <0, 0, 0>
    angle 58
    }

    background { color rgb<1,1,1>*0.03 }


    light_source { <-20, 30, -25> color red 0.6 green 0.6 blue 0.6 }
    light_source { < 20, 30, -25> color red 0.6 green 0.6 blue 0.6 }
    
    #declare m = 1;
    union {

    union {
    sphere { m*<-1.232479216269295e-18,-5.0683195292765995e-18,0.999134264384709>, 1 }        
    sphere {  m*<-2.1993917943521975e-19,-4.321027165333219e-18,5.280134264384736>, 1 }
    sphere {  m*<9.428090415820634,2.648258797907677e-21,-2.334199068948623>, 1 }
    sphere {  m*<-4.714045207910317,8.16496580927726,-2.334199068948623>, 1}
    sphere { m*<-4.714045207910317,-8.16496580927726,-2.334199068948623>, 1 }    

        pigment { color rgb<0.8,0,0>  }
    finish { ambient 0.1 diffuse 0.7 phong 1 }
    }    
    union {
    cylinder { m*<-2.1993917943521975e-19,-4.321027165333219e-18,5.280134264384736>, <-1.232479216269295e-18,-5.0683195292765995e-18,0.999134264384709>, 0.5 }
    cylinder { m*<9.428090415820634,2.648258797907677e-21,-2.334199068948623>, <-1.232479216269295e-18,-5.0683195292765995e-18,0.999134264384709>, 0.5}
    cylinder { m*<-4.714045207910317,8.16496580927726,-2.334199068948623>, <-1.232479216269295e-18,-5.0683195292765995e-18,0.999134264384709>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.334199068948623>, <-1.232479216269295e-18,-5.0683195292765995e-18,0.999134264384709>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
    finish { ambient 0.1 diffuse 0.7 phong 1 }    
