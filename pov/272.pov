    cylinder { m*<-4.714045207910317,8.16496580927726,-2.984794493317328>, <-5.428922562885084e-18,5.99595525585813e-20,0.34853884001600316>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.984794493317328>, <-5.428922562885084e-18,5.99595525585813e-20,0.34853884001600316>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
