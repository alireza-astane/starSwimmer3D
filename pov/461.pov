    cylinder { m*<-4.714045207910317,8.16496580927726,-2.750805217607683>, <-2.3101490481636877e-18,-2.386175238373873e-18,0.5825281157256503>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.750805217607683>, <-2.3101490481636877e-18,-2.386175238373873e-18,0.5825281157256503>, 0.5}

