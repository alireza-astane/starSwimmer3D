    sphere { m*<-4.714045207910317,-8.16496580927726,-2.499960031193716>, 1 }    

        pigment { color rgb<0.8,0,0>  }
    finish { ambient 0.1 diffuse 0.7 phong 1 }
    }    
    union {
    cylinder { m*<-7.638683565525181e-20,-5.599822448760516e-18,6.12937330213964>, <2.402898516632651e-18,-5.374982529829739e-18,0.8333733021396181>, 0.5 }
    cylinder { m*<9.428090415820634,8.78820561305375e-20,-2.499960031193716>, <2.402898516632651e-18,-5.374982529829739e-18,0.8333733021396181>, 0.5}
    cylinder { m*<-4.714045207910317,8.16496580927726,-2.499960031193716>, <2.402898516632651e-18,-5.374982529829739e-18,0.8333733021396181>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.499960031193716>, <2.402898516632651e-18,-5.374982529829739e-18,0.8333733021396181>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
    finish { ambient 0.1 diffuse 0.7 phong 1 }    
