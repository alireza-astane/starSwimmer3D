    cylinder { m*<-4.714045207910317,8.16496580927726,-2.62400862199466>, <6.127813777014229e-19,-3.957999962747192e-18,0.7093247113386737>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.62400862199466>, <6.127813777014229e-19,-3.957999962747192e-18,0.7093247113386737>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
