    cylinder { m*<-4.714045207910317,8.16496580927726,-3.0629488375444165>, <-2.43336567708959e-18,1.5751780819008837e-18,0.2703844957889167>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-3.0629488375444165>, <-2.43336567708959e-18,1.5751780819008837e-18,0.2703844957889167>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
