    cylinder { m*<-4.714045207910317,8.16496580927726,-2.889892938752911>, <-4.469658581800925e-18,-2.6824994387015483e-20,0.44344039458042106>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.889892938752911>, <-4.469658581800925e-18,-2.6824994387015483e-20,0.44344039458042106>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
