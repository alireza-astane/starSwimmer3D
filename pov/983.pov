#version 3.7; 
    global_settings { assumed_gamma 1.0 }
    

    camera {
    location  <20, 20, 20>
    right     x*image_width/image_height
    look_at   <0, 0, 0>
    angle 58
    }

    background { color rgb<1,1,1>*0.03 }


    light_source { <-20, 30, -25> color red 0.6 green 0.6 blue 0.6 }
    light_source { < 20, 30, -25> color red 0.6 green 0.6 blue 0.6 }
    
    #declare m = 1;
    union {

    union {
    sphere { m*<-2.625290236508491e-18,-5.583820154376381e-18,1.1792337824995192>, 1 }        
    sphere {  m*<-5.386888642565231e-18,-5.601531065580317e-18,4.2912337824995666>, 1 }
    sphere {  m*<9.428090415820634,4.589355046484177e-20,-2.154099550833814>, 1 }
    sphere {  m*<-4.714045207910317,8.16496580927726,-2.154099550833814>, 1}
    sphere { m*<-4.714045207910317,-8.16496580927726,-2.154099550833814>, 1 }    

        pigment { color rgb<0.8,0,0>  }
    finish { ambient 0.1 diffuse 0.7 phong 1 }
    }    
    union {
    cylinder { m*<-5.386888642565231e-18,-5.601531065580317e-18,4.2912337824995666>, <-2.625290236508491e-18,-5.583820154376381e-18,1.1792337824995192>, 0.5 }
    cylinder { m*<9.428090415820634,4.589355046484177e-20,-2.154099550833814>, <-2.625290236508491e-18,-5.583820154376381e-18,1.1792337824995192>, 0.5}
    cylinder { m*<-4.714045207910317,8.16496580927726,-2.154099550833814>, <-2.625290236508491e-18,-5.583820154376381e-18,1.1792337824995192>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.154099550833814>, <-2.625290236508491e-18,-5.583820154376381e-18,1.1792337824995192>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
    finish { ambient 0.1 diffuse 0.7 phong 1 }    
    }
    
