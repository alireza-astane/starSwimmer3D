    cylinder { m*<-4.714045207910317,8.16496580927726,-3.106071625612491>, <-2.51340532145083e-18,2.2523511365887456e-18,0.22726170772084234>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-3.106071625612491>, <-2.51340532145083e-18,2.2523511365887456e-18,0.22726170772084234>, 0.5}

