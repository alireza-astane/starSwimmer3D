    cylinder { m*<-4.714045207910317,8.16496580927726,-2.1738445165360383>, <-1.1076261405690892e-18,-5.380949821403035e-18,1.1594888167972945>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.1738445165360383>, <-1.1076261405690892e-18,-5.380949821403035e-18,1.1594888167972945>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
