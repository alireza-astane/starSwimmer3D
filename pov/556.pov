    cylinder { m*<-4.714045207910317,8.16496580927726,-2.6359730558259935>, <5.595961134685997e-20,-3.937365554040851e-18,0.6973602775073403>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.6359730558259935>, <5.595961134685997e-20,-3.937365554040851e-18,0.6973602775073403>, 0.5}

    pigment { color rgb<0.6,.2,.2>  }
