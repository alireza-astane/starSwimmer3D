    cylinder { m*<-4.714045207910317,8.16496580927726,-2.165503740394214>, <-2.1218121309973183e-18,-5.455889568974295e-18,1.1678295929391187>, 0.5 }
    cylinder {  m*<-4.714045207910317,-8.16496580927726,-2.165503740394214>, <-2.1218121309973183e-18,-5.455889568974295e-18,1.1678295929391187>, 0.5}

